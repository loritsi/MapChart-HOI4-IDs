{
    "manifest_version": 2,
    "name": "MapChart HoI4 ID Tooltips",
    "version": "1.2",
    "description": "Appends state IDs to state names in HoI4 MapChart tooltips.",
    "content_scripts": [
      {
        "matches": ["https://www.mapchart.net/hearts-of-iron-iv.html"],
        "js": ["contentScript.js"]
      }
    ],
    "permissions": [
      "activeTab"
    ],
    "web_accessible_resources": [
      "list.json"
    ]
  }
  